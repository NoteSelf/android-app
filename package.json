{
  "name": "noteself",
  "version": "1.0.0",
  "description": "noteself mobile apps",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm run build && cordova run android --device",
    "dev-legacy": "cordova plugin add cordova-plugin-crosswalk-webview && cordova clean && npm run dev",
    "icons": "cordova-icon",
    "clean": "rimraf www/bundle.js www/index.html",
    "build": "npm run clean && npm run tw && webpack",
    "webpack": "npm run clean && webpack",
    "watch": "webpack --watch",
    "tw": "cross-env TIDDLYWIKI_PLUGIN_PATH='../../tiddlywiki/plugins_dist;tw-plugins/dist' npm run TW",
    "TW": "tiddlywiki ./tw --verbose --build index jsFiles",
    "TW:JS": "tiddlywiki ./tw --verbose --build jsFiles"
  },
  "author": "Danielo Rodr√≠guez Rivero",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "bitbar-webpack-progress-plugin": "^0.1.3",
    "cordova-icon": "^0.9.1",
    "cross-env": "^3.1.4",
    "ts-loader": "^2.0.0",
    "webpack": "^2.2.0",
    "typescript": "^2.4.2"
  },
  "dependencies": {
    "cordova-custom-config": "^3.1.2",
    "lodash.memoize": "^4.1.2",
    "rimraf": "^2.6.1",
    "tiddlywiki": "^5.1.14"
  }
}